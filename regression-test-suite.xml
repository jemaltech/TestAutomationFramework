<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression test Suite" parallel="classes" thread-count="2">
    <test name="Login-page test" group-by-instances="true">
        <classes>
            <class name="LoginPageTest">
            <methods><include name="SignInWithValidUserCredential"/>
            </methods>
        </class>
        </classes>
    </test>
    <test name="Home-page test" group-by-instances="true">
        <classes>
            <class name="HomePageTest">
                <methods>
                    <include name="searchFriendsNameUsingFacebookSearch"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>