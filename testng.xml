<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Daily Regression Suite" parallel="tests" thread-count="4">
    <test name="Home Page Tests">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="HomePageTest" />
        </classes>
    </test>
    <test name="Login Page Tests">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="LoginPageTest" />
        </classes>
    </test>
</suite>